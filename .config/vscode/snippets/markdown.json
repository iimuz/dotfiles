{
	"hugo frontmatter yaml": {
		"prefix": [
			"frontmatter",
			"hugo"
		],
		"body": [
			"---",
			"title: \"${1:title}\"",
			"date: \"${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}T${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}\"",
			"draft: ${2:true}",
			"tags:\n  - ${3:tagname}",
			"math: ${4:false}",
			"image: \"${5:image url}\"",
			"key: ${6:key name}",
			"link:\n  - ${7:key link}",
			"---"
		],
		"description": "hugo custom header."
	},
	"hugo key link": {
		"prefix": [
			"hugo",
			"keylink"
		],
		"body": [
			"{{< key-link ${1:key name} >}}"
		],
		"description": "hugo custom shortcode for key link."
	},
	"hugo table of contents": {
		"prefix": [
			"hugo",
			"toc"
		],
		"body": [
			"Table of Contents",
			"{{< toc >}}{{< /toc >}}"
		],
		"description": "hugo table of contents."
	}
}
