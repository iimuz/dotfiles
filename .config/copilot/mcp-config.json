{
  "mcpServers": {
    "atlassian": {
      "tools": ["getConfluencePage", "getJiraIssue"],
      "type": "local",
      "command": "mcp-remote",
      "args": ["https://mcp.atlassian.com/v1/sse"]
    },
    "awslabs_aws-documentation-mcp-server": {
      "type": "local",
      "command": "awslabs.aws-documentation-mcp-server",
      "args": [],
      "tools": [
        "get_available_services",
        "read_documentation",
        "recommend",
        "search_documentation"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR",
        "AWS_DOCUMENTATION_PARTITION": "aws"
      }
    },
    "markitdown": {
      "tools": ["convert_to_markdown"],
      "type": "local",
      "command": "markitdown-mcp",
      "args": []
    }
  }
}
